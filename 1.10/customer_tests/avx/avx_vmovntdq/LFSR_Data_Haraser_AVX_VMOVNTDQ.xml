<!-- <ExpectedValue>OK</ExpectedValue> -->

<IntelligentMemoryChecker>

	<IMC_configuration>
		<flow>data_harasser</flow>
		<iterations>1</iterations>
		<continue_on_fail>false</continue_on_fail>
	</IMC_configuration>

	<data_harasser>
		<flow_type>DATA_HARASSER</flow_type>
		<iterations>15</iterations>
		<algorithm>lfsr_data</algorithm>
		<opcode>AVX_VMOVNTDQ</opcode>
		<memory_block>memory_block_1</memory_block>
	</data_harasser>

	<!--Algorithms configuration-->

	<lfsr_data>
		<algorithm_type>LFSR</algorithm_type>
		<limit_pattern>0xFFFFFFFFFFFFFFFF</limit_pattern>
		<seed>0x123456789abc</seed>
	</lfsr_data>

	<memory_block_1>
		<memory_block_type>MALLOC</memory_block_type>
		<size_in_bytes>4096</size_in_bytes>
	</memory_block_1>

</IntelligentMemoryChecker>
