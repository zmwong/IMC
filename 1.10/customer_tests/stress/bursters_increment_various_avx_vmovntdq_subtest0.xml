	<increment_address_algorithm_burst_4>
		<algorithm_type>INCREMENT</algorithm_type>
		<incrementor>128</incrementor>
	</increment_address_algorithm_burst_4>

	<increment_address_algorithm_burst_5>
		<algorithm_type>INCREMENT</algorithm_type>
		<incrementor>128</incrementor>
	</increment_address_algorithm_burst_5>

	<increment_address_algorithm_burst_6>
		<algorithm_type>INCREMENT</algorithm_type>
		<incrementor>128</incrementor>
	</increment_address_algorithm_burst_6>

	<increment_address_algorithm_burst_7>
		<algorithm_type>INCREMENT</algorithm_type>
		<incrementor>128</incrementor>
	</increment_address_algorithm_burst_7>


<!--
	No memory blocks configuration due to this file meant to be used by Runner
-->

</IntelligentMemoryChecker>

