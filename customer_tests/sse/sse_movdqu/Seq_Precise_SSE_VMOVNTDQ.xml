<!-- <ExpectedValue>OK</ExpectedValue> -->

<IntelligentMemoryChecker>

	<!--IMC flows queue configuration-->

	<IMC_configuration>
		<flow>burster_flow_1</flow>
		<iterations>10</iterations>
	</IMC_configuration>

	<!--Flows configuration-->

	<burster_flow_1>
		<flow_type>BURSTER</flow_type>
		<iterations>1</iterations>
		<data_bursts>2</data_bursts>
		<address_algorithm>increment_address_algorithm_burst</address_algorithm>
		<data_algorithm>pattern_list_algorithm</data_algorithm>
		<opcode>SSE_MOVDQU</opcode>
		<memory_block>memory_block_1</memory_block>
		<same_address>true</same_address>
	</burster_flow_1>

	<!--Algorithms configuration-->

	<pattern_list_algorithm>
		<algorithm_type>PATTERN_LIST</algorithm_type>
		<pattern_list>0xffffffffffffffff</pattern_list>
		<pattern_list>0xffffffff00000000</pattern_list>
		<pattern_list>0xffff0000ffff0000</pattern_list>
		<pattern_list>0xff00ff00ff00ff00</pattern_list>
		<pattern_list>0xf0f0f0f0f0f0f0f0</pattern_list>
		<pattern_list>0xcccccccccccccccc</pattern_list>
		<pattern_list>0xaaaaaaaaaaaaaaaa</pattern_list>
	</pattern_list_algorithm>

	<increment_address_algorithm_burst>
		<algorithm_type>INCREMENT</algorithm_type>
		<incrementor>16</incrementor>
	</increment_address_algorithm_burst>

	<!--Memory blocks configuration-->

	<memory_block_1>
		<memory_block_type>MALLOC</memory_block_type>
		<size_in_bytes>4096</size_in_bytes>
	</memory_block_1>

</IntelligentMemoryChecker>
