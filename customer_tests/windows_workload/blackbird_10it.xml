<!-- <ExpectedValue>OK</ExpectedValue> -->

<IntelligentMemoryChecker>

	<!--IMC flows queue configuration-->
	<IMC_configuration>
		<flow>blackbird_flow</flow>
		<continue_on_fail>false</continue_on_fail>
		<iterations>10</iterations>
	</IMC_configuration>

	<!--Flows configuration-->
	<blackbird_flow>
		<flow_type>BLACKBIRD</flow_type>
		<address_algorithm>address_algorithm</address_algorithm>
		<data_algorithm>lfsr_data_algorithm</data_algorithm>
		<opcode>LEGACY</opcode>
		<memory_block>memory_block_1</memory_block>
	</blackbird_flow>

	<!--Algorithms configuration-->

	<address_algorithm>
		<algorithm_type>INCREMENT</algorithm_type>
		<incrementor>8</incrementor>
	</address_algorithm>

	<lfsr_data_algorithm>
		<algorithm_type>LFSR</algorithm_type>
		<seed>0x123456789abc</seed>
	</lfsr_data_algorithm>

	<!--Memory blocks configuration-->

	<memory_block_1>
		<memory_block_type>MALLOC</memory_block_type>
		<size_in_bytes>4096</size_in_bytes>
	</memory_block_1>

</IntelligentMemoryChecker>
