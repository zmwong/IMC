<!-- <ExpectedValue>OK</ExpectedValue> -->

<IntelligentMemoryChecker>

	<IMC_configuration>
		<flow>data_harasser</flow>
		<flow>burster_flow</flow>
		<iterations>1</iterations>
		<continue_on_fail>false</continue_on_fail>
	</IMC_configuration>

	<!--Flows configuration-->

	<burster_flow>
		<flow_type>BURSTER</flow_type>
		<iterations>50</iterations>
		<data_algorithm>lfsr_data</data_algorithm>
		<address_algorithm>increment_address</address_algorithm>
		<opcode>AVX512_VMOVDQU</opcode>
		<data_bursts>16</data_bursts>
		<same_address>false</same_address>
		<memory_block>memory_block_1</memory_block>
	</burster_flow>

	<data_harasser>
		<flow_type>DATA_HARASSER</flow_type>
		<iterations>1</iterations>
		<algorithm>init_algorithm</algorithm>
		<opcode>AVX512_VMOVDQU</opcode>
		<memory_block>memory_block_1</memory_block>
	</data_harasser>

	<!--Algorithms configuration-->

	<init_algorithm>
		<algorithm_type>CONSTANT</algorithm_type>
	</init_algorithm>

	<increment_address>
		<algorithm_type>INCREMENT</algorithm_type>
		<incrementor>1024</incrementor>
	</increment_address>

	<lfsr_data>
		<algorithm_type>LFSR</algorithm_type>
		<seed>0x123456789abc</seed>
	</lfsr_data>

	<memory_block_1>
		<memory_block_type>MALLOC</memory_block_type>
		<size_in_bytes>4096</size_in_bytes>
	</memory_block_1>

</IntelligentMemoryChecker>
